<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/cl_first_onboarding"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".presentation.ui.onboarding.SecondOnboardingActivity">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/cl_first_onboarding_top"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/gray_black"
            app:layout_constraintTop_toTopOf="parent">

            <ImageView
                android:id="@+id/iv_first_onboarding_back"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:layout_marginTop="4dp"
                android:layout_marginBottom="4dp"
                android:padding="10dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/ic_icn_back" />

        </androidx.constraintlayout.widget.ConstraintLayout>


        <View
            android:id="@+id/view_first_onboarding_progressbar_green"
            android:layout_width="0dp"
            android:layout_height="4dp"
            android:background="@drawable/rectangle_fill_green_radius_0"
            app:layout_constraintEnd_toStartOf="@+id/view_first_onboarding_progressbar_gray"
            app:layout_constraintHorizontal_weight="6"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/cl_first_onboarding_top" />


        <View
            android:id="@+id/view_first_onboarding_progressbar_gray"
            android:layout_width="0dp"
            android:layout_height="4dp"
            android:background="@drawable/rectangle_fill_gray3_radius_0"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_weight="3"
            app:layout_constraintStart_toEndOf="@+id/view_first_onboarding_progressbar_green"
            app:layout_constraintTop_toBottomOf="@+id/cl_first_onboarding_top" />

        <TextView
            android:id="@+id/tv_first_onboarding_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:layout_marginTop="24dp"
            android:fontFamily="@font/pretendard_regular"
            android:lineSpacingExtra="4dp"
            android:text="@string/onboarding_start_thunder"
            android:textColor="@color/gray_black"
            android:textSize="22sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/view_first_onboarding_progressbar_green" />


        <TextView
            android:id="@+id/tv_firt_onboarding_request_mbti"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/pretendard_regular"
            android:lineSpacingExtra="4dp"
            android:text="@string/onboarding_select_option"
            android:layout_marginTop="8dp"
            android:textColor="@color/gray_black02"
            android:textSize="14sp"
            app:layout_constraintStart_toStartOf="@+id/tv_first_onboarding_name"
            app:layout_constraintTop_toBottomOf="@+id/tv_first_onboarding_name" />


        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/cl_open_not_select"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="100dp"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="20dp"
            android:background="@drawable/rectangle_border_gray01_radius_10"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_firt_onboarding_request_mbti">

            <TextView
                android:id="@+id/tv_onboarding_open"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/pretendard_bold"
                android:text="@string/text_open"
                android:textColor="@color/gray_gray01"
                android:textSize="16sp"
                android:layout_marginTop="23dp"
                android:layout_marginStart="24dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/tv_onboarding_open_message"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="6dp"
                android:fontFamily="@font/pretendard_medium"
                android:text="@string/onboarding_open_message"
                android:textColor="@color/gray_gray01"
                android:textSize="14sp"
                android:layout_marginBottom="23dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="@+id/tv_onboarding_open"
                app:layout_constraintTop_toBottomOf="@+id/tv_onboarding_open" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/cl_join_select"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="20dp"
            android:layout_marginTop="10dp"
            android:background="@drawable/rectangle_fill_black01_radius_10"
            android:visibility="invisible"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/cl_open_not_select">

            <TextView
                android:id="@+id/tv_onboarding_join_select"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="24dp"
                android:layout_marginTop="23dp"
                android:fontFamily="@font/pretendard_bold"
                android:text="@string/text_join"
                android:textColor="@color/main_green"
                android:textSize="16sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/tv_onboarding_join_message_select"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="6dp"
                android:layout_marginBottom="23dp"
                android:fontFamily="@font/pretendard_medium"
                android:text="@string/onboarding_join_message"
                android:textColor="@color/white"
                android:textSize="14sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="@+id/tv_onboarding_join_select"
                app:layout_constraintTop_toBottomOf="@+id/tv_onboarding_join_select" />

        </androidx.constraintlayout.widget.ConstraintLayout>


        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/cl_join_not_select"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="20dp"
            android:layout_marginTop="10dp"
            android:background="@drawable/rectangle_border_gray01_radius_10"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/cl_open_not_select">

            <TextView
                android:id="@+id/tv_onboarding_join"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="24dp"
                android:layout_marginTop="23dp"
                android:fontFamily="@font/pretendard_bold"
                android:text="@string/text_join"
                android:textColor="@color/gray_gray01"
                android:textSize="16sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/tv_onboarding_join_message"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="6dp"
                android:layout_marginBottom="23dp"
                android:fontFamily="@font/pretendard_medium"
                android:text="@string/onboarding_join_message"
                android:textColor="@color/gray_gray01"
                android:textSize="14sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="@+id/tv_onboarding_join"
                app:layout_constraintTop_toBottomOf="@+id/tv_onboarding_join" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/cl_open_select"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="100dp"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="20dp"
            android:visibility="invisible"
            android:background="@drawable/rectangle_fill_black01_radius_10"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_firt_onboarding_request_mbti">

            <TextView
                android:id="@+id/tv_onboarding_open_select"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/pretendard_bold"
                android:text="@string/text_open"
                android:textColor="@color/main_green"
                android:textSize="16sp"
                android:layout_marginTop="23dp"
                android:layout_marginStart="24dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/tv_onboarding_open_message_select"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="6dp"
                android:fontFamily="@font/pretendard_medium"
                android:text="@string/onboarding_open_message"
                android:textColor="@color/white"
                android:textSize="14sp"
                android:layout_marginBottom="23dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="@+id/tv_onboarding_open_select"
                app:layout_constraintTop_toBottomOf="@+id/tv_onboarding_open_select" />

        </androidx.constraintlayout.widget.ConstraintLayout>



        <TextView
            android:id="@+id/tv_first_onboarding_next"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="36dp"
            android:background="@drawable/rectangle_fill_gray03_border_gray02_radius_10"
            android:fontFamily="@font/pretendard_bold"
            android:paddingTop="19dp"
            android:paddingBottom="19dp"
            android:text="@string/text_next"
            android:textAlignment="center"
            android:textColor="@color/gray_gray01"
            android:textSize="15sp"
            android:visibility="visible"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="20dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />




    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>